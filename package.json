{
  "name": "next-tailwind-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build-static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:styles": "stylelint --fix \"./src/**/*.module.css\"",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "vercel": "vercel --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "next start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test --silent -- --coverage",
    "test:coverage:only": "npx jest \"$npm_config_src\" \"--coverage\" \"--collectCoverageFrom='$npm_config_src/**/*.(ts|tsx)'\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.5",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "acd-utils": "8.0.0",
    "autoprefixer": "10.4.7",
    "await-to-js": "3.0.0",
    "classcat": "5.0.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-testing-library": "5.5.1",
    "eslint-webpack-plugin": "3.2.0",
    "husky": "8.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "28.1.2",
    "jest-watch-typeahead": "2.0.0",
    "lint-staged": "13.0.3",
    "next": "12.2.2",
    "nprogress": "0.2.0",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "postcss-nesting": "10.1.10",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-transition-group": "4.4.2",
    "stylelint": "14.9.1",
    "stylelint-config-concentric-order": "5.0.0",
    "stylelint-config-standard": "26.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "3.1.5",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "typescript": "4.7.4",
    "vercel": "^19.1.2"
  }
}
