{
  "name": "next-tailwind-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build-static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:styles": "stylelint --fix \"./src/**/*.module.css\"",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "vercel": "vercel --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "next start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test --silent -- --coverage",
    "test:coverage:only": "npx jest \"$npm_config_src\" \"--coverage\" \"--collectCoverageFrom='$npm_config_src/**/*.(ts|tsx)'\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.13",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.4",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "acd-utils": "8.0.0",
    "autoprefixer": "10.4.2",
    "await-to-js": "3.0.0",
    "classcat": "5.0.3",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-testing-library": "5.0.4",
    "eslint-webpack-plugin": "3.1.1",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.7",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.3.2",
    "next": "12.1.0",
    "nprogress": "0.2.0",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "postcss-nesting": "10.1.2",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss":"0.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-transition-group": "4.4.2",
    "stylelint": "14.3.0",
    "stylelint-config-concentric-order": "5.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "3.0.18",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "vercel": "23.1.2"
  }
}
